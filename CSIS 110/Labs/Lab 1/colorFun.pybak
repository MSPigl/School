# colorFun.py:  some fun functions for manipulating color pictures

def fun1 (picture):
  for p in getPixels (picture):
    value = getRed(p)
    setRed (p, value * 0.5)
    
def loveGreen (picture):
  for p in getPixels (picture):
    value = getGreen(p)
    setGreen (p, 255)
    
def fun2 (picture):
  for p in getPixels (picture):
    value = getRed(p)
    setRed (p, value * 2)