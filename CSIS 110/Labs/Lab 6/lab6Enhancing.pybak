# Doubles positive values, minimizes negative values
def increaseVolume (sound):
  for sample in getSamples (sound):
    value = getSample (sample)
    if value>= 0:
      setSampleValue (sample, value*2)
    if value < 0:
      setSampleValue (sample, -32768)  
    
      

      
         